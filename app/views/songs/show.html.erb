<h1><%= @song.name %></h1>
Artist: <%= @song.artist %>
Genre: <%= @song.genre %>
Likes: <%= @song.likes %>
<%= youtube_video @song.url %>
<%= link_to "Edit Song Information", edit_song_path(@song)%>
<%= link_to "Song Index", songs_path %>
<%= button_to "Add Like", song_path(@song), method: :post %>
<%= button_to "Delete Song", song_path(@song), method: :delete %>