<%= form_for @song do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.select :artist, @artists %>
    <%= f.select :genre, @genres %>
    <%= f.label "Embed URL Link" %>
    <%= f.text_field :url%>
    <%= f.submit %>
<% end %>

